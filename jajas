  {
 "cells": [
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "359b4169-2ddc-4ff7-9444-f15c089716fe",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.interpolate import lagrange"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "e1a71b48-703a-4029-ac71-ceec46535d29",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "coefx = np.array([-1, 0, 1, 3])\n",
    "coefy = np.array([-2, 1, -4, 10])\n",
    "x = np.linspace(-1.5, 3.1, 200)\n",
    "\n",
    "polilag = lagrange(coefx, coefy)\n",
    "\n",
    "fig = plt.figure(figsize=(10,8))\n",
    "plt.scatter(coefx, coefy, s=200, c='r')\n",
    "plt.plot(x, polilag(x), 'b', lw='3')\n",
    "\n",
    "plt.title('Polinomio de lagrange(scipy)', fontsize=20)\n",
    "plt.grid()\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('y')\n",
    "\n",
    "plt.show()"
   ]
  }
